{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build OpenWrt Release",
            "type": "shell",
            "command": "make clean-openwrt && make build-openwrt",
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}